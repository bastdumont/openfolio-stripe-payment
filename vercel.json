{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    },
    {
      "src": "open_folio_multilingual_landing_fr_de_en_with_i_18_n.html",
      "use": "@vercel/static"
    },
    {
      "src": "stripe_payment_page.html", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.py"
    }
  ],
  "env": {
    "STRIPE_SECRET_KEY": "@stripe_secret_key",
    "PORT": "4242"
  },
  "functions": {
    "server.py": {
      "maxDuration": 30
    }
  }
}
