{
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "server.py"
    },
    {
      "src": "/payment",
      "dest": "server.py"
    },
    {
      "src": "/privacy",
      "dest": "server.py"
    },
    {
      "src": "/terms",
      "dest": "server.py"
    },
    {
      "src": "/(.*)",
      "dest": "server.py"
    }
  ]
}